#include <iostream>
#include <vector>

int main() {
  int product_odd = 1;
  int total_even = 0;
  
  std::vector<int> numbers = {2, 4, 3, 6, 1, 9};
  
  for (int i = 0; i < numbers.size(); i++) {
    
    if (numbers[i] % 2 == 0) {
      
    total_even = total_even + numbers[i];
  }
  else{
    
    product_odd = product_odd * numbers[i];
  }
  }
  std::cout << "Sum of even: " << total_even << "\n";
  std::cout << "Product of odd: " << product_odd << "\n";
}
